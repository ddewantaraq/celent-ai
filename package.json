  "description": "",
  "engines": {
    "node": ">=20.9.0"
  },
  "dependencies": {
    "@ai-sdk/groq": "^1.2.9",
    "@mastra/core": "^0.10.10",
    "@mastra/libsql": "^0.11.0",
    "@mastra/loggers": "^0.10.3",
    "@mastra/mcp": "^0.10.5",
    "@mastra/memory": "^0.11.1",
    "@types/express": "^4.17.21",
    "@types/validator": "^13.15.2",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.0.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.10",
    "mastra": "^0.10.10",
    "sequelize-cli": "^6.6.3",
    "typescript": "^5.8.3"
  }
}
